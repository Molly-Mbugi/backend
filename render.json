{
    "builds": [
      { "src": "index.js", "use": "@vercel/node" }
    ],
    "routes": [
      {
        "src": "/.*",
        "dest": "index.js"
      }
    ],
    "services": [
      {
        "name": "your-service-name",
        "buildCommand": "npm install && npm run build",
        "startCommand": "npm start",
        "env": {
          "NODE_ENV": "production"
        },
        "files": [
          {
            "source": "db.json",
            "destination": "/app/db.json"
          }
        ]
      }
    ]
  }
  